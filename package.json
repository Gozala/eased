{
  "name": "eased",
  "version": "0.1.0",
  "description": "functional style easing & animations library with type safety",
  "keywords": [
    "easeing",
    "interpolation",
    "animation",
    "bezier",
    "elm",
    "functional",
    "typed",
    "flow"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/eased",
  "main": "./lib/index.js",
  "dependencies": {
    "value-color": "0.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-plugin-syntax-flow": "6.3.13",
    "babel-plugin-transform-es2015-modules-umd": "6.4.3",
    "babel-plugin-transform-flow-strip-types": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "flow-bin": "0.22.1",
    "tap": "1.1.0",
    "tape": "4.4.0"
  },
  "scripts": {
    "test": "npm run flow && npm run tape",
    "tape": "tape -r babel-register test/**/*.js",
    "flow": "flow check",
    "start": "babel --watch --out-dir ./lib ./src",
    "build-node": "babel --out-dir ./lib ./src && cp ./src/*.flow ./lib",
    "build-browser": "mkdir -p dist && babel --out-file ./dist/eased.js --plugins 'transform-es2015-modules-umd' ./src",
    "build": "npm run build-node && npm run build-browser",
    "prepublish": "npm run build && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/eased.git",
    "web": "https://github.com/Gozala/eased"
  },
  "bugs": {
    "url": "https://github.com/Gozala/eased/issues/"
  },
  "license": "MIT"
}
